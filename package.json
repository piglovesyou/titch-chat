{
  "name": "titch-chat",
  "version": "0.0.0",
  "scripts": {
    "start": "NODE_ENV=production node ./lib/main",
    "dev": "BABEL_DISABLE_CACHE=1 nodemon --watch src/backend --exec babel-node -- ./src/main",
    "debug": "BABEL_DISABLE_CACHE=1 babel-node debug ./src/main",
    "test": "BABEL_DISABLE_CACHE=1 wdio wdio.conf.js",
    "build:server": "BABEL_DISABLE_CACHE=1 babel -d lib src",
    "build:client": "webpack --config webpack-config/client.js",
    "build": "npm run build:server && npm run build:client",
    "prepublish": "NODE_ENV=production npm run build"
  },
  "engines": {
    "node": "6.x"
  },
  "description": "React server-side rendering with CSS modules example",
  "dependencies": {
    "compression": "^1.6.2",
    "connect-redis": "^3.1.0",
    "cookie-parser": "^1.4.3",
    "express": "~4.13.1",
    "express-session": "^1.14.1",
    "is-browser": "^2.0.1",
    "moment": "^2.15.0",
    "morgan": "~1.6.1",
    "passport": "^0.3.2",
    "passport-google-oauth": "^1.0.0",
    "passport-oauth": "^1.0.0",
    "q": "^1.4.1",
    "react": "^15.2.0",
    "react-dom": "^15.2.0",
    "react-icons": "^2.2.1",
    "react-modal": "^1.4.0",
    "serve-favicon": "~2.3.0",
    "synceddb-client": "0.0.8",
    "synceddb-persistence-memory": "0.0.3",
    "synceddb-persistence-postgres": "0.0.2",
    "synceddb-server": "0.0.5"
  },
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-core": "^6.7.2",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-async-to-generator": "^6.7.4",
    "babel-plugin-transform-class-properties": "^6.6.0",
    "babel-plugin-transform-es2015-classes": "^6.7.7",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.7.7",
    "babel-plugin-webpack-loaders": "^0.7.1",
    "babel-preset-react": "^6.5.0",
    "babel-register": "^6.11.6",
    "compression-webpack-plugin": "^0.3.1",
    "css-loader": "^0.23.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "flux": "^2.1.1",
    "glob": "^7.0.5",
    "node-sass": "^3.10.0",
    "react-router": "^2.6.0",
    "sass-loader": "^4.0.0",
    "wdio-mocha-framework": "^0.4.0",
    "wdio-selenium-standalone-service": "0.0.5",
    "webdriverio": "^4.2.3",
    "webpack": "^1.13.1",
    "webpack-dev-middleware": "^1.6.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/piglovesyou/react-server-side-rendering-with-css-modules.git"
  },
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/piglovesyou/react-server-side-rendering-with-css-modules/issues"
  },
  "homepage": "https://github.com/piglovesyou/react-server-side-rendering-with-css-modules#readme",
  "main": "lib/main",
  "directories": {
    "test": "test"
  },
  "keywords": [
    "chat"
  ],
  "author": "thepiglovesyou@gmail.com"
}
